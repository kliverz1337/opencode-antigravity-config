{
    "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
    // ============================================================
    // AGENT OVERRIDES - ALL GEMINI PRO
    // Semua agent menggunakan Gemini 3.1 Pro
    // ============================================================
    "agents": {
        "sisyphus": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "high",
            "temperature": 0.7,
            "maxTokens": 32000
        },
        "prometheus": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "high",
            "temperature": 0.6,
            "maxTokens": 16000
        },
        "hephaestus": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "standard",
            "temperature": 0.5,
            "maxTokens": 32000
        },
        "metis": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "high",
            "temperature": 0.7,
            "maxTokens": 32000
        },
        "momus": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "standard",
            "temperature": 0.5,
            "maxTokens": 32000
        },
        "atlas": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "standard",
            "temperature": 0.5,
            "maxTokens": 32000
        },
        "oracle": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "high",
            "temperature": 0.5,
            "maxTokens": 32000
        },
        "librarian": {
            "model": "google/antigravity-gemini-3-flash",
            "variant": "low",
            "temperature": 0.3,
            "maxTokens": 16000
        },
        "explore": {
            "model": "google/antigravity-gemini-3-flash",
            "variant": "minimal",
            "temperature": 0.2,
            "maxTokens": 8000,
            "permission": {
                "edit": "deny",
                "bash": "ask"
            }
        },
        "multimodal-looker": {
            "model": "google/antigravity-gemini-3-flash",
            "variant": "standard",
            "temperature": 0.5,
            "maxTokens": 8000
        }
    },
    // ============================================================
    // CATEGORIES
    // ============================================================
    "categories": {
        "visual-engineering": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "high",
            "temperature": 0.9,
            "maxTokens": 32000
        },
        "ultrabrain": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "high",
            "temperature": 0.7,
            "maxTokens": 32000
        },
        "deep": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "high",
            "temperature": 0.7,
            "maxTokens": 32000
        },
        "artistry": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "high",
            "temperature": 1.0,
            "maxTokens": 32000
        },
        "quick": {
            "model": "google/antigravity-gemini-3-flash",
            "variant": "minimal",
            "temperature": 0.3,
            "maxTokens": 8000
        },
        "unspecified-low": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "standard",
            "temperature": 0.7,
            "maxTokens": 16000
        },
        "unspecified-high": {
            "model": "google/antigravity-gemini-3-1-pro",
            "variant": "high",
            "temperature": 0.7,
            "maxTokens": 32000
        },
        "writing": {
            "model": "google/antigravity-gemini-3-flash",
            "variant": "medium",
            "temperature": 0.7,
            "maxTokens": 16000
        }
    },
    // ============================================================
    // SISYPHUS AGENT (ORCHESTRATOR)
    // ============================================================
    "sisyphus_agent": {
        "disabled": false,
        "default_builder_enabled": false,
        "planner_enabled": true,
        "replace_plan": true
    },
    // ============================================================
    // BACKGROUND TASKS
    // ============================================================
    "background_task": {
        "defaultConcurrency": 5,
        "staleTimeoutMs": 180000,
        "providerConcurrency": {
            "google": 5
        },
        "modelConcurrency": {
            "google/antigravity-gemini-3-flash": 5,
            "google/antigravity-gemini-3-1-pro": 4
        }
    },
    // ============================================================
    // GIT MASTER
    // ============================================================
    "git_master": {
        "commit_footer": false,
        "include_co_authored_by": false
    },
    // ============================================================
    // HOOKS & COMMANDS
    // ============================================================
    "disabled_hooks": [
        "startup-toast"
    ],
    // ============================================================
    // EXPERIMENTAL FEATURES
    // ============================================================
    "experimental": {
        "aggressive_truncation": false,
        "auto_resume": true,
        "truncate_all_tool_outputs": false,
        "dynamic_context_pruning": {
            "enabled": false
        }
    }
}